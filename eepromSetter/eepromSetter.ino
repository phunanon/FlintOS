const uint8_t bin [] PROGMEM = {
  0xFF
}; //HT
const uint16_t entry = 0x000;

void setup ()
{
  uint8_t len = sizeof(bin) / sizeof(*bin);


  for (uint16_t b = 0; b < len; ++b) EEPROM.update(entry + b, pgm_read_byte(&bin[b]));
}

void loop () {}
